<main class='inner landing'>
  <h1 class='landing-heading'>Sign In</h1>
  <br/>
  <div class='lead container'>
    <%= form_for :session, url: '/sessions/' do |form| %>
      <div class='form-group'>
        <%= form.text_field :email, placeholder: 'Email' %>
      </div>
      <div class='form-group'>
        <%= form.password_field :password, placeholder: 'Password' %>
      </div>
      <%= form.submit 'Sign In', class: 'btn btn-secondary'%>
    <% end %>
  </div>
</main>
